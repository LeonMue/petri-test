struct NonDeletedFieldStruct {
    int32 myInt32;
    string hello;
    bool isNotDeleted;
    float64 myFloat64;
    string world;
}

struct DeletedFieldStruct {
    int32 myInt32;
    @deleted string hello;
    @deleted bool isNotDeleted;
    @deleted float64 myFloat64;
    string world;
    @deleted float32 neverSeen;
}